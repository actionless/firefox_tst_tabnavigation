{
	"manifest_version": 2,
	"name": "tst_tabnavigation",
	"version": "0.5",
	"description": "Improved keyboard navigtion for Tree Style Tab",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"commands": {
	  "toggle-keynavigation": {
		"suggested_key": {
		  "default": "Ctrl+Shift+L"
		},
		"description": "Show keyboard navigation overlay"
	  }
	},

	"permissions": [
		"storage",
		"tabs"
	],

	"browser_specific_settings": {
	  "gecko": {
		"id": "{9d2b1d4e-04e3-4ec2-9ed2-bee38fc09b8b}"
	  }
	},

	"background": {
		"scripts": ["background.js"]
	}

}
